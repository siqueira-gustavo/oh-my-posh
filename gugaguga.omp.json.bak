{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": false,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{.Host}} in {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#26C6DA",
          "properties": {
            "prefix": "",
            "postfix": "",
            "macos": "mac"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "lightGreen",
          "properties": {
            "postfix": " <#ffffff>in</> ",
            "template": "{{ .ComputerName }}"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "invert_powerline": false,
          "foreground": "yellow",
          "properties": {
            "prefix": "<#26C6DA>î—¿ </>/",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "invert_powerline": true,
          "properties": {
            "prefix": "<#ffffff>on</> ",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "properties": {
            "prefix": "",
            "text": "<#CB4B16>\u276F</><#CACA02>\u276F</><#4E9A06>\u276F</>"
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#7650e7",
          "background": "#b69ffd",
          "properties": {
            "prefix": "\u005b\uE235 ",
            "postfix": "\u005D",
            "display_mode": "files",
            "fetch_virtual_env": false,
            "template": "{{ .Full }}"
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "foreground": "#7FD5EA",
          "properties": {
            "prefix": "\u005b\uFCD1 ",
            "postfix": "\u005D",
            "display_version": true
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "#1c4711",
          "background": "#76b367",
          "properties": {
            "prefix": "\u005b\uE718 ",
            "postfix": "\u005D",
            "display_version": true
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "#ea2d2e",
          "background": "#db9f9f",
          "properties": {
            "prefix": "\u005b\uE738 ",
            "postfix": "\u005D",
            "display_version": true
          }
        },
        {
          "type": "aws",
          "style": "powerline",
          "foreground": "#ffffff",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "properties": {
            "prefix": "\u005b\uE7AD ",
            "postfix": "\u005D",
            "display_default": false
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "invert_powerline": true,
          "foreground": "#000000",
          "background": "#c4a000",
          "properties": {
            "always_enabled": true,
            "postfix": " \uF252 "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "invert_powerline": true,
          "background": "#d3d7cf",
          "foreground": "#000000",
          "properties": {
            "time_format": "Mon, 02 Jan 2006 15:04:05",
            "postfix": " \uF017 "
          }
        },
        {
          "type": "battery",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "properties": {
            "discharging_icon": "\uE231 ",
            "charging_icon": "\uE234 ",
            "charged_icon": "\uE22F ",
            "postfix": " \uf583 ",
            "template": "{{ if not .Error }}{{.Icon}}{{.Percentage}}{{ end }}"
          }
        }
      ]
    }
  ]
}